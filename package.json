    "@fortawesome/react-native-fontawesome": "^0.1.0",
    "@hot-loader/react-dom": "^16.9.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "nconf": "^0.10.0",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "react": "16.8.1",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-native": "0.62.3",
    "react-native-paper": "^2.16.0",
